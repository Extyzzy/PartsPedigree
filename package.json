{
  "name": "PartsPedigree",
  "version": "1.16.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "link": "node node_modules/react-native/local-cli/cli.js link",
    "emulate-ios-iphonex": "react-native run-ios --simulator=\"iPhone X\"",
    "emulate_iphone_se": "react-native run-ios --simulator=\"iPhone SE\"",
    "run-ipad": "node node_modules/react-native/local-cli/cli.js run-ios --simulator='iPad Air'",
    "flow": "flow",
    "run-android": "node node_modules/react-native/local-cli/cli.js run-android",
    "log-android": "node node_modules/react-native/local-cli/cli.js log-android",
    "generate_splash": "yo rn-toolbox:assets --splash assets/splash.png",
    "generate_icon": "yo rn-toolbox:assets --icon assets/icon.png",
    "release-android": "cd android && ./gradlew assembleRelease",
    "release-android-uat": "cd android && ENV_TYPE=uat ./gradlew assembleRelease",
    "release-android-production": "cd android && ENV_TYPE=production ./gradlew assembleRelease",
    "release-android-clean": "cd android && ./gradlew clean",
    "emulate-ios": "react-native run-ios",
    "emulate-android": "(adb uninstall com.partspedigree.app || 1) && react-native run-android",
    "run-nexus": "/Users/nk/Library/Android/sdk/tools/emulator -avd Nexus_5_API_24"
  },
